<div class="container">
<table class="table">


      <thead>
            <tr>
                  <th scope="col">No</th>
                  <th scope="col">Nama</th>
                  <th scope="col">No HP</th>
                  <th scope="col">Actiion</th>
            </tr>
      </thead>

   
      <tbody>
            <% contacts.forEach((contact,i) =>  { %>
             
           
            <tr>
                  <th scope="row"><%=i+1%></th>
                  <td><%=contact.name %></td>
                  <td><%=contact.noHP %></td>
                  <td></td>
            </tr>
<% }) %>
      </tbody>
</table>
</div>
